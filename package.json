{
  "name": "electron-forge",
  "version": "6.0.0-beta.57",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">= 12.13.0"
  },
  "workspaces": {
    "packages": [
      "packages/api/*",
      "packages/installer/*",
      "packages/maker/*",
      "packages/publisher/*",
      "packages/utils/*",
      "packages/plugin/*",
      "packages/template/*"
    ]
  },
  "scripts": {
    "clean": "rimraf dist && lerna exec -- rimraf dist",
    "prebuild": "yarn clean && ts-node tools/link-ts.ts && ts-node tools/copy-shared-types.ts && tsc --project ./tsconfig.packages.json --emitDeclarationOnly && ts-node tools/copy-types.ts",
    "build": "yarn build:quick",
    "build:quick": "lerna exec -- babel src -d dist --quiet --extensions \".ts\" --config-file ../../../.babelrc",
    "postbuild": "ts-node tools/test-dist",
    "commit": "git-cz",
    "coverage:fast": "cross-env INTEGRATION_TESTS=0 TS_NODE_FILES=true nyc mocha './tools/test-globber.ts' && nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:slow": "cross-env TS_NODE_FILES=true nyc mocha './tools/test-globber.ts' --integration && nyc report --reporter=text-lcov > coverage.lcov",
    "docs": "yarn docs:generate && yarn docs:position",
    "docs:generate": "lerna exec -- node_modules/.bin/typedoc --out doc --excludeExternals --ignoreCompilerErrors --mode file --excludePrivate --excludeProtected --hideGenerator",
    "docs:position": "ts-node tools/position-docs.ts",
    "docs:deploy": "ts-node tools/sync-readmes.ts && yarn docs && ts-node tools/copy-now.ts && cd docs && now && now alias",
    "docs:deploy:ci": "ts-node tools/sync-readmes.ts && yarn docs && ts-node tools/copy-now.ts && cd docs && now --token $NOW_TOKEN && now alias --token $NOW_TOKEN",
    "lint": "eslint --ext .ts .",
    "test": "cross-env TS_NODE_FILES=true yarn run mocha './tools/test-globber.ts'",
    "test:fast": "cross-env TEST_FAST_ONLY=1 TS_NODE_FILES=true yarn run mocha './tools/test-globber.ts'",
    "postinstall": "rimraf node_modules/.bin/*.ps1 && ts-node tools/link-ts.ts"
  },
  "dependencies": {
    "@electron-forge/async-ora": "file:packages/utils/async-ora",
    "@electron-forge/cli": "file:packages/api/cli",
    "@electron-forge/core": "file:packages/api/core",
    "@electron-forge/installer-base": "file:packages/installer/base",
    "@electron-forge/installer-darwin": "file:packages/installer/darwin",
    "@electron-forge/installer-deb": "file:packages/installer/deb",
    "@electron-forge/installer-dmg": "file:packages/installer/dmg",
    "@electron-forge/installer-exe": "file:packages/installer/exe",
    "@electron-forge/installer-linux": "file:packages/installer/linux",
    "@electron-forge/installer-rpm": "file:packages/installer/rpm",
    "@electron-forge/installer-zip": "file:packages/installer/zip",
    "@electron-forge/maker-appx": "file:packages/maker/appx",
    "@electron-forge/maker-base": "file:packages/maker/base",
    "@electron-forge/maker-deb": "file:packages/maker/deb",
    "@electron-forge/maker-dmg": "file:packages/maker/dmg",
    "@electron-forge/maker-flatpak": "file:packages/maker/flatpak",
    "@electron-forge/maker-pkg": "file:packages/maker/pkg",
    "@electron-forge/maker-rpm": "file:packages/maker/rpm",
    "@electron-forge/maker-snap": "file:packages/maker/snap",
    "@electron-forge/maker-squirrel": "file:packages/maker/squirrel",
    "@electron-forge/maker-wix": "file:packages/maker/wix",
    "@electron-forge/maker-zip": "file:packages/maker/zip",
    "@electron-forge/plugin-auto-unpack-natives": "file:packages/plugin/auto-unpack-natives",
    "@electron-forge/plugin-base": "file:packages/plugin/base",
    "@electron-forge/plugin-compile": "file:packages/plugin/compile",
    "@electron-forge/plugin-electronegativity": "file:packages/plugin/electronegativity",
    "@electron-forge/plugin-local-electron": "file:packages/plugin/local-electron",
    "@electron-forge/plugin-webpack": "file:packages/plugin/webpack",
    "@electron-forge/publisher-base": "file:packages/publisher/base",
    "@electron-forge/publisher-bitbucket": "file:packages/publisher/bitbucket",
    "@electron-forge/publisher-electron-release-server": "file:packages/publisher/electron-release-server",
    "@electron-forge/publisher-github": "file:packages/publisher/github",
    "@electron-forge/publisher-nucleus": "file:packages/publisher/nucleus",
    "@electron-forge/publisher-s3": "file:packages/publisher/s3",
    "@electron-forge/publisher-snapcraft": "file:packages/publisher/snapcraft",
    "@electron-forge/shared-types": "file:packages/utils/types",
    "@electron-forge/template-base": "file:packages/template/base",
    "@electron-forge/template-typescript": "file:packages/template/typescript",
    "@electron-forge/template-typescript-webpack": "file:packages/template/typescript-webpack",
    "@electron-forge/template-webpack": "file:packages/template/webpack",
    "@electron-forge/test-utils": "file:packages/utils/test-utils",
    "@electron-forge/web-multi-logger": "file:packages/utils/web-multi-logger"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@malept/cross-spawn-promise": "^1.1.1",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cross-spawn": "^6.0.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/express-ws": "^3.0.0",
    "@types/fetch-mock": "^7.3.1",
    "@types/fs-extra": "^9.0.6",
    "@types/glob": "^7.1.3",
    "@types/inquirer": "^7.3.0",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.166",
    "@types/mime-types": "^2.1.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^8.0.4",
    "@types/node": "^15.3.0",
    "@types/node-fetch": "^2.5.5",
    "@types/proxyquire": "^1.3.28",
    "@types/semver": "^7.3.4",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-plugin-source-map-support": "^2.1.2",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.0.0",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.2",
    "cz-customizable": "^6.3.0",
    "electron-installer-common": "^0.10.2",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-mocha": "^9.0.0",
    "fetch-mock": "^9.10.7",
    "generate-changelog": "^1.8.0",
    "globby": "^11.0.1",
    "husky": "^6.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "listr": "^0.14.1",
    "minimist": "^1.2.5",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.3",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.1",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "ts-node": "^10.0.0",
    "typedoc": "^0.20.29",
    "typescript": "~4.0.2"
  },
  "optionalDependencies": {
    "@malept/electron-installer-flatpak": "^0.11.2",
    "electron-installer-debian": "^3.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-installer-redhat": "^3.2.0",
    "electron-installer-snap": "^5.1.0",
    "electron-windows-store": "^2.1.0",
    "electron-winstaller": "^5.0.0",
    "electron-wix-msi": "^3.0.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz.js"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --ext .ts --fix"
  },
  "nyc": {
    "exclude": [
      "packages/*/*/test/**/*.{js,ts}",
      "tools/*.ts"
    ],
    "extends": "@istanbuljs/nyc-config-typescript"
  }
}
